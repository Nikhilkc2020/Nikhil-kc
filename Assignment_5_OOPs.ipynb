    "class OOPs:\n",
    "    def __init__(self, number, times):\n",
    "        self.num = number\n",
    "        self.tim = times\n",
    "        \n",
    "    def mul(self):\n",
    "        x = 1\n",
    "        for i in range(self.tim):\n",
    "            x*=self.num\n",
    "        return x"
   ]
